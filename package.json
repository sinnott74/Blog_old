{
  "name": "sinnott",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "npm run start:prod",
    "postinstall": "cd ./webclient && npm run build:prod",
    "start:prod": "cd ./webclient && npm run build:prod && cd ../server && npm run start:prod",
    "start:dev": "concurrently --kill-others \"npm run start:webclient:dev\" \"npm run start:server:dev\"",
    "start:webclient:dev": " cd ./webclient && pwd && npm run start:dev",
    "start:server:dev": "pwd && cd ./server && npm run start:dev",
    "test": "echo \"Error: no test specified\" && exit 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "sinnott-server": "file:server",
    "sinnott-webclient": "file:webclient"
  },
  "devDependencies": {
    "concurrently": "^3.5.1",
    "webpack-dev-server": "^2.9.2",
    "nodemon": "^1.14.11"
  }
}
